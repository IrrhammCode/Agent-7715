
# ==============================================
# AGENT 7715 CONFIGURATION TEMPLATE
# ==============================================

# ----------------------------------------------
# 1. CORE AUTHENTICATION (BACKEND)
# ----------------------------------------------
# Private Key for the Agent/Executor Account.
# This account must have Sepolia ETH to pay for transaction gas.
AGENT_PRIVATE_KEY=<your_agent_private_key_here>

# ----------------------------------------------
# 2. NETWORK & CONTRACTS (ETHEREUM SEPOLIA)
# ----------------------------------------------
RPC_URL=https://ethereum-sepolia.publicnode.com
CHAIN_ID=11155111

# The custom AgentRouter contract (Verified on Sepolia)
AGENT_ROUTER_ADDRESS=0x690ab1758ae6c99857a3241d18da0ffdd6c7c7ae

# Token Addresses (USDC / DEMO)
USDC_ADDRESS=0xc970a9C00AEAf314523B9B289F6644CcCbfE6930
WETH_ADDRESS=0xfFf9976782d46CC05630D1f6eBAb18b2324d6B14

# Uniswap V3 SwapRouter (Sepolia)
UNISWAP_SWAP_ROUTER_ADDRESS=0x3bFA4769FB09eefC5a80d6E87c3B9C650f7Ae48E

# ----------------------------------------------
# 3. EXTERNAL SERVICES
# ----------------------------------------------

# [Email Notifications]
# Use a Gmail App Password (not your login password)
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_gmail_app_password

# [Envio HyperSync]
# Optional. Used for fetching high-speed blockchain history.
# Get a token at https://envio.dev if rate limited.
ENVIO_API_TOKEN=your_envio_api_token_here

# ----------------------------------------------
# 4. FRONTEND CONFIGURATION
# ----------------------------------------------

# URL of the Backend API
NEXT_PUBLIC_BACKEND_URL=http://localhost:3001
NEXT_PUBLIC_WS_URL=ws://localhost:3001/ws

# Public Address of the AGENT_PRIVATE_KEY above (Used for permission target)
NEXT_PUBLIC_AGENT_ADDRESS=<your_agent_public_address_here>

# WalletConnect Project ID (Required for AppKit/Wagmi)
# Get free at https://cloud.walletconnect.com
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_project_id_here

# ----------------------------------------------
# 5. SERVER SETTINGS
# ----------------------------------------------
PORT=3001